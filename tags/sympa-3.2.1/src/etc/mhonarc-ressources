<SubjectHeader>
<TABLE border=0 width=100%>
<TR bgcolor="--BG_COLOR--">
 <TD Valign=top>
  <FONT size=+1 COLOR="--DARK_COLOR--"><B> $SUBJECTNA$</B></FONT><FONT size=+1> $FROMNAME$ </FONT>
 </TD>

<TD ALIGN=RIGHT NOWRAP>
[IF user->email]
<!-- reply to button -->
<FORM ACTION="[path_cgi]" METHOD=POST>
  <INPUT TYPE="hidden" NAME="in_reply_to" VALUE="$MSGID$">
  <INPUT TYPE="hidden" NAME="subject" VALUE="Re: $SUBJECTNA$">
  <INPUT TYPE="hidden" NAME="action" VALUE="compose_mail">
  <INPUT TYPE="hidden" NAME="list" VALUE="[list]">
[IF lang=fr]
   répondre à l'auteur <INPUT TYPE="radio" NAME="to" VALUE="$FROMADDR$" CHECKED>
  à la liste <INPUT TYPE="radio" NAME="to" VALUE="[list]@[list->host]">
 <INPUT TYPE="submit" NAME="action_compose_mail" VALUE="Répondre">
[ELSE] 
  Reply to sender <INPUT TYPE="radio" NAME="to" VALUE="$FROMADDR$" CHECKED>
  to list <INPUT TYPE="radio" NAME="to" VALUE="[list]@[list->host]">
  <INPUT TYPE="submit" NAME="action_compose_mail" VALUE="Reply">

[ENDIF]
</FORM>
[ENDIF]

[IF action<>viewmod]
[IF user->email=$FROMADDR$]
  <FORM ACTION="[path_cgi]" METHOD=POST>
  <INPUT TYPE="hidden" NAME="list" VALUE="[list]">
  <INPUT TYPE="hidden" NAME="yyyy" VALUE="$yyyy$">
  <INPUT TYPE="hidden" NAME="month" VALUE="$mois$">
  <INPUT TYPE="hidden" NAME="msgid" VALUE="$MSGID$">
[IF lang=fr]
  <INPUT TYPE="submit" NAME="action_remove_arc" VALUE="marquer ce message pour suppression">
[ELSIF lang=cz]
  <INPUT TYPE="submit" NAME="action_remove_arc" VALUE="Oznaèit tuto zprávu ke smazání">
[ELSE]
  <INPUT TYPE="submit" NAME="action_remove_arc" VALUE="tag this mail for deletion">
[ENDIF]
  </FORM>
[ELSIF is_owner]
  <FORM ACTION="[path_cgi]" METHOD=POST>
  <INPUT TYPE="hidden" NAME="list" VALUE="[list]">
  <INPUT TYPE="hidden" NAME="yyyy" VALUE="$yyyy$">
  <INPUT TYPE="hidden" NAME="month" VALUE="$mois$">
  <INPUT TYPE="hidden" NAME="msgid" VALUE="$MSGID$">
  <INPUT TYPE="hidden" NAME="action" VALUE="remove_arc">

[IF lang=fr]
  <INPUT TYPE="button" NAME="action_remove_arc" VALUE="marquer ce message pour suppression" onClick="request_confirm(this.form,'Voulez-vous vraiment supprimer ce message ?')">
[ELSIF lang=cz]
  <INPUT TYPE="submit" NAME="action_remove_arc" VALUE="oznaèit tuto zprávu ke smazání">
[ELSE]
  <INPUT TYPE="button" NAME="action_remove_arc" VALUE="tag this mail for deletion" onClick="request_confirm(this.form,'Do you really want to delete this message ?')">
[ENDIF]
  </FORM>
[ENDIF]

[ENDIF]
 </TD>
 </TR>
</TABLE>

